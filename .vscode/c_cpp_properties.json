{
    "configurations": [
        {
            "name": "Mac",
            "browse": {
                "path": [
                    "${workspaceFolder}"
                ],
                "limitSymbolsToIncludedHeaders": true
            },
            "includePath": [
                "${workspaceFolder}",
                "${workspaceFolder}/",
                "${workspaceFolder}/Dependencies",
                "${workspaceFolder}/Dependencies/lua",
                "${workspaceFolder}/Dependencies/protobuf/src",
                "${workspaceFolder}/Dependencies/libevent/include"
            ],
            "defines": ["DEBUG"],
            "macFrameworkPath": [
                "/System/Library/Frameworks",
                "/Library/Frameworks"
            ],
            "compilerPath": "/usr/bin/clang",
            "cStandard": "c11",
            "cppStandard": "c++11",
            "intelliSenseMode": "clang-x64"
        }
    ],
    "version": 4
}