<XML>
	<Propertys>
		<Property Id="PushID" Type="string" Public="false" Private="true" Save="false" Index="0" SaveInterval="0" ScriptFunction="" Desc="上次推送的ID"/>
		<Property Id="ActiveID" Type="string" Public="false" Private="true" Save="false" Index="0" SaveInterval="0" ScriptFunction="" Desc="当前活动ID(活动是否开启)"/>
		<Property Id="JoinPVPActive" Type="int" Public="false" Private="true" Save="false" Index="0" SaveInterval="0" ScriptFunction="" Desc="自己是否参与了PVP活动"/>
		<Property Id="PVPTitle" Type="string" Public="false" Private="true" Save="false" Index="0" SaveInterval="0" ScriptFunction="" Desc="PVP标题ID=MatchData,根据积分算的"/>
		
		<Property Id="WinCount" Type="int" Public="false" Private="true" Save="true" Index="0" SaveInterval="0" ScriptFunction="" Desc="今日连胜次数"/>		
		<Property Id="TodayScore" Type="int" Public="false" Private="true" Save="true" Index="0" SaveInterval="0" ScriptFunction="" Desc="今日积分(必须有小组赛才有)--TotalScore总积分"/>
		<Property Id="GroupLevel" Type="int" Public="false" Private="false" Save="false" Index="0" SaveInterval="0" ScriptFunction="" Desc="上次小组(排名)"/>
		
		<!--<Property Id="PVP_POWER" Type="int" Public="false" Private="true" Save="true" Index="0" SaveInterval="0" ScriptFunction="" Desc="上次对阵的是否比你强弱EPVP_NONE"/>-->

	</Propertys>
	<Records>
		<Record Id="PVPList" Row="3" Col="3" Public="false" Private="true" Save="false" Index="0" SaveInterval="0" ScriptFunction="" Desc="普通背包">
			<Col Type="object" Desc="匹配的3个PVP对象ID"/>
			<Col Type="int" Desc="强弱EPVP_NONE"/>
			<Col Type="int" Desc="是否选择开打"/>
		</Record>
	</Records>
	<Includes>	
	</Includes>
