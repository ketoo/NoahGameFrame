<XML>
	<Propertys>
		<!--基础数据-->
		<Property Id="RoleName" Type="string" Public="true" Private="true" Save="true" Index="0" SaveInterval="0" RelationValue="Friend" Desc="角色名"/>
		<Property Id="Sex" Type="int" Public="true" Private="true" Save="true" Index="0" SaveInterval="0" RelationValue="Friend" Desc="性别"/>
		<Property Id="Race" Type="int" Public="true" Private="true" Save="true" Index="0" SaveInterval="0" RelationValue="Friend" Desc="种族"/>
		<Property Id="Camp" Type="int" Public="true" Private="true" Save="true" Index="0" SaveInterval="0" RelationValue="Friend" Desc="阵营"/>
		<Property Id="LastContainerID" Type="int" Public="false" Private="true" Save="true" Index="0" SaveInterval="0" RelationValue="Friend" Desc="玩家下线的时候需要保存上次在线的场景"/>
		
		<Property Id="Level" Type="int" Public="true" Private="true" Save="true" Index="0" SaveInterval="0" RelationValue="Friend" Desc="等级,属性名不能超过20字符,重要的是，分层的属性，不能保存，因为表会自动保存"/>
		<Property Id="ShowName" Type="string" Public="true" Private="true" Save="false" Index="0" SaveInterval="0" RelationValue="Friend" Desc="显示的名字"/>
		<Property Id="PrefabPath" Type="string" Public="false" Private="true" Save="false" Index="0" SaveInterval="0" RelationValue="Friend" Desc="预设路径"/>
		<Property Id="FirstTarget" Type="object" Public="false" Private="true" Save="false" Index="0" SaveInterval="0" RelationValue="Friend" Desc="首要目标"/>
		<Property Id="State" Type="int" Public="true" Private="true" Save="false" Index="0" SaveInterval="0" RelationValue="Friend" Desc="0呆着1跑2走3闪烁4技能飞移5死6平常技能中"/>
		<Property Id="CharType" Type="int" Public="false" Private="false" Save="false" Index="0" SaveInterval="0" RelationValue="Friend" Desc="角色类型"/>
		<Property Id="Job" Type="int" Public="true" Private="true" Save="true" Index="0" SaveInterval="0" RelationValue="Friend" Desc="职业"/>
		<!--变量数据-->
		<Property Id="EXP" Type="int" Public="false" Private="true" Save="true" Index="0" SaveInterval="0" RelationValue="Friend" Desc="经验获得,如果是怪物，则是掉落经验"/>
		<Property Id="HP" Type="int" Public="false" Private="true" Save="false" Index="0" SaveInterval="0" RelationValue="Friend" Desc="生命值"/>
		<Property Id="SP" Type="int" Public="false" Private="true" Save="true" Index="0" SaveInterval="0" RelationValue="Friend" Desc="体力"/>
		<Property Id="MP" Type="int" Public="false" Private="true" Save="true" Index="0" SaveInterval="0" RelationValue="Friend" Desc="法力值"/>
		<Property Id="Money" Type="int" Public="false" Private="true" Save="true" Index="0" SaveInterval="0" RelationValue="Friend" Desc="钱"/>
		<!--平常数据-->
		<Property Id="Account" Type="string" Public="false" Private="false" Save="true" Index="0" SaveInterval="0" RelationValue="Friend" Desc="玩家的帐号"/>
		<Property Id="ConnectKey" Type="string" Public="false" Private="false" Save="false" Index="0" SaveInterval="0" RelationValue="Friend" Desc="玩家连接服务器的KEY"/>
		<Property Id="Gold" Type="int" Public="false" Private="false" Save="true" Index="0" SaveInterval="0" RelationValue="Friend" Desc="玩家金钱"/>
		<Property Id="MAXEXP" Type="int" Public="false" Private="true" Save="true" Index="0" SaveInterval="0" RelationValue="Friend" Desc="升级需要的经验"/>
		<Property Id="DEAD_COUNT" Type="int" Public="false" Private="true" Save="true" Index="0" SaveInterval="0" RelationValue="Friend" Desc="死亡次数--需要保存的都是因为不分层的属性，分层属性不需要保存，保存各层的值即可"/>
