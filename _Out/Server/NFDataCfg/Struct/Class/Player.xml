		<Property Id="ATK_LIGHT" Type="int" Public="false" Private="true" Save="false" Index="0" SaveInterval="0" RelationValue="Friend" Desc="雷电"/>
		<Property Id="ATK_ICE" Type="int" Public="false" Private="true" Save="false" Index="0" SaveInterval="0" RelationValue="Friend" Desc="冰"/>
		<Property Id="ATK_POISON" Type="int" Public="false" Private="true" Save="false" Index="0" SaveInterval="0" RelationValue="Friend" Desc="毒"/>
		<Property Id="DEF_FIRE" Type="int" Public="false" Private="true" Save="false" Index="0" SaveInterval="0" RelationValue="Friend" Desc="火抗"/>
		<Property Id="DEF_LIGHT" Type="int" Public="false" Private="true" Save="false" Index="0" SaveInterval="0" RelationValue="Friend" Desc="雷电抗"/>
		<Property Id="DEF_ICE" Type="int" Public="false" Private="true" Save="false" Index="0" SaveInterval="0" RelationValue="Friend" Desc="冰抗"/>
		<Property Id="DEF_POISON" Type="int" Public="false" Private="true" Save="false" Index="0" SaveInterval="0" RelationValue="Friend" Desc="毒抗"/>
		<Property Id="DIZZY_GATE" Type="int" Public="true" Private="true" Save="false" Index="0" SaveInterval="0" RelationValue="Friend" Desc="眩晕开关"/>
		<Property Id="MOVE_GATE" Type="int" Public="true" Private="true" Save="false" Index="0" SaveInterval="0" RelationValue="Friend" Desc="移动开关"/>
		<Property Id="SKILL_GATE" Type="int" Public="true" Private="true" Save="false" Index="0" SaveInterval="0" RelationValue="Friend" Desc="技能开关"/>
		<Property Id="PHYSICAL_GATE" Type="int" Public="true" Private="true" Save="false" Index="0" SaveInterval="0" RelationValue="Friend" Desc="物理免疫开关"/>
