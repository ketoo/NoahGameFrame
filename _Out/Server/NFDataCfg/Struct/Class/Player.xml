			<Col Type="int" Tag="MAXHP"/>
			<Col Type="int" Tag="MAXMP"/>
			<Col Type="int" Tag="MAXSP"/>				
			<Col Type="int" Tag="HPREGEN"/>
			<Col Type="int" Tag="SPREGEN"/>
			<Col Type="int" Tag="MPREGEN"/>		
