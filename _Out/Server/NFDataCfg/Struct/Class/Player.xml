		<Property Id="OnlineTime" Type="object" Public="false" Private="false" Save="false" Index="0" SaveInterval="0" RelationValue="Friend" Desc="本次上线时间"/>
		<Property Id="TotalLineTime" Type="object" Public="false" Private="false" Save="false" Index="0" SaveInterval="0" RelationValue="Friend" Desc="今日在线时长{截至至本次上线时间}"/>
		<Property Id="GMLevel" Type="int" Public="false" Private="true" Save="false" Index="0" SaveInterval="0" RelationValue="Friend" Desc="GM等级"/>
		<Property Id="LastAttacker" Type="object" Public="false" Private="false" Save="false" Index="0" SaveInterval="0" RelationValue="Friend" />
		
		<!--战斗数据,可以影响BUFF叠加-->
		<Property Id="MAXHP" Type="int" Public="false" Private="true" Save="false" Index="0" SaveInterval="0" RelationValue="Friend" Desc="生命值"/>
		<Property Id="MAXMP" Type="int" Public="false" Private="true" Save="false" Index="0" SaveInterval="0" RelationValue="Friend" Desc="法力值"/>
		<Property Id="MAXSP" Type="int" Public="false" Private="true" Save="false" Index="0" SaveInterval="0" RelationValue="Friend" Desc="体力"/>
