		<Property Id="X" Type="float" Public="false" Private="false" Save="true" Index="0" SaveInterval="0" RelationValue="Friend" />
		<Property Id="Y" Type="float" Public="false" Private="false" Save="true" Index="0" SaveInterval="0" RelationValue="Friend" />
		<Property Id="Z" Type="float" Public="false" Private="false" Save="true" Index="0" SaveInterval="0" RelationValue="Friend" />
