		<Property Id="LUCKLY_CARD" Type="string" Public="false" Private="true" Save="true" Index="0" SaveInterval="0" RelationValue="Friend" Desc="佩戴的幸运卡ID"/>
		<Property Id="ATK_PVP" Type="int" Public="false" Private="true" Save="true" Index="0" SaveInterval="0" RelationValue="Friend" Desc="PVP攻击力"/>
