	<servlet-mapping>
		<servlet-name>VmwIncludeHelperServlet</servlet-name>
		<url-pattern>/servletincludehelper</url-pattern>
	</servlet-mapping>

	<session-config>
		<session-timeout>20</session-timeout>
	</session-config>

	<welcome-file-list>
		<welcome-file>Default.aspx</welcome-file>
	</welcome-file-list>

</web-app>
